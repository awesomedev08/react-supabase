{
    "name": "react-supabase",
    "version": "0.1.5",
    "description": "React Hooks library for Supabase",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist/**"
    ],
    "repository": "tmm/react-supabase",
    "homepage": "https://react-supabase.vercel.app",
    "license": "MIT",
    "scripts": {
        "build": "vite build && tsc --emitDeclarationOnly",
        "build:watch": "concurrently \"vite build --watch\" \"tsc --emitDeclarationOnly --watch\"",
        "clean": "rimraf dist",
        "lint": "eslint . --cache",
        "lint:fix": "yarn lint --fix",
        "lint:format": "prettier --write .",
        "lint:types": "tsc --noEmit",
        "prepare": "husky install .config/husky",
        "prepublishOnly": "yarn clean && yarn build",
        "test": "jest"
    },
    "peerDependencies": {
        "@supabase/supabase-js": "^1.11.6",
        "react": "^17.0.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@commitlint/config-conventional": "^12.1.1",
        "@supabase/postgrest-js": "^0.28.1",
        "@supabase/supabase-js": "^1.11.6",
        "@testing-library/react-hooks": "^5.1.1",
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.41",
        "@types/react": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "commitlint": "^12.1.1",
        "concurrently": "^6.0.2",
        "eslint": "^7.24.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest-dom": "^3.8.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-testing-library": "^4.1.0",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "jest-watch-typeahead": "^0.6.3",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.5",
        "typescript": "^4.2.4",
        "vite": "^2.2.1"
    }
}
